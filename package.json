{
  "name": "cf-container-warm-pool",
  "version": "0.1.1",
  "description": "A warm pool manager for Cloudflare Containers - pre-warm containers and acquire them on-demand",
  "type": "module",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "cloudflare",
    "containers",
    "workers",
    "warm-pool",
    "durable-objects"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@cloudflare/containers": ">=0.0.1",
    "@cloudflare/workers-types": ">=4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@cloudflare/containers": "^0.1.0",
    "@cloudflare/workers-types": "^4.20260212.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
